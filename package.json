{
  "name": "acorn-test262",
  "version": "1.0.0",
  "author": "Boshen",
  "license": "MIT",
  "main": "index.mjs",
  "type": "module",
  "scripts": {
    "init": "pnpm run '/^clone-*/'",
    "clone-test262": "just clone-submodule submodules/test262 https://github.com/tc39/test262.git a5e69a1534de88d1eb29b76657d84c8541b72df7",
    "clone-acorn-jsx": "just clone-submodule submodules/acorn-jsx https://github.com/acornjs/acorn-jsx.git 8ed96d6ddec2065204ba07d924bb2e7bca539ea6",
    "clone-typescript": "just clone-submodule submodules/typescript https://github.com/microsoft/TypeScript.git e9bcbe6ef706e0b5a34678964988eb6a9cd86cc6",
    "start": "pnpm run gen-test262 && pnpm run gen-acorn-jsx && pnpm run gen-typescript",
    "gen-test262": "node src/test262.js",
    "gen-acorn-jsx": "node src/acorn-jsx.js",
    "gen-typescript": "node src/typescript-eslint.js",
    "lint": "oxlint src",
    "fmt": "dprint fmt"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^8.35.1",
    "acorn": "8.15.0",
    "dprint": "^0.50.1",
    "estree-walker": "^3.0.3",
    "meriyah": "^6.1.4",
    "oxlint": "^1.12.0",
    "yaml": "^2.7.0"
  },
  "packageManager": "pnpm@10.17.0"
}
