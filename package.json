{
  "name": "acorn-test262",
  "version": "1.0.0",
  "author": "Boshen",
  "license": "MIT",
  "main": "index.mjs",
  "type": "module",
  "scripts": {
    "init": "pnpm run '/^clone-*/'",
    "clone-test262": "just clone-submodule submodules/test262 https://github.com/tc39/test262.git 26058a01fdbc8dad9ded0e97133190098ea8c5d8",
    "clone-acorn-jsx": "just clone-submodule submodules/acorn-jsx https://github.com/acornjs/acorn-jsx.git 8ed96d6ddec2065204ba07d924bb2e7bca539ea6",
    "clone-typescript": "just clone-submodule submodules/typescript https://github.com/microsoft/TypeScript.git 669c25c091ad4d32298d0f33b0e4e681d46de3ea",
    "start": "pnpm run gen-test262 && pnpm run gen-acorn-jsx && pnpm run gen-typescript",
    "gen-test262": "node src/test262.js",
    "gen-acorn-jsx": "node src/acorn-jsx.js",
    "gen-typescript": "node src/typescript-eslint.js",
    "lint": "oxlint src",
    "fmt": "oxfmt"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^8.48.0",
    "acorn": "^8.15.0",
    "estree-walker": "^3.0.3",
    "meriyah": "^6.1.4",
    "oxfmt": "^0.15.0",
    "oxlint": "^1.12.0",
    "yaml": "^2.7.0"
  },
  "packageManager": "pnpm@10.23.0"
}
